<VirtualHost *:80>
    ServerName breakermind.com
    ServerAlias breakermind.com
    ServerAdmin hello@breakermind.com
    DocumentRoot /var/www/html/breakermind.com
    ErrorLog /var/www/logs/virtual.host.breakermind.com.error.log
    CustomLog /var/www/logs/virtual.host.breakermind.com.access.log combined
</VirtualHost>

<VirtualHost *:80>
    ServerName breakermind.com
    ServerAlias freshi.breakermind.com
    ServerAdmin hello@breakermind.com
    DocumentRoot /var/www/html/freshi.breakermind.com
    ErrorLog /var/www/logs/virtual.host.freshi.breakermind.com.error.log
    CustomLog /var/www/logs/virtual.host.freshi.breakermind.com.access.log combined
</VirtualHost>

<VirtualHost *:80>
